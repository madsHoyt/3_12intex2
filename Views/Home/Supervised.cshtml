@{
    ViewData["Title"] = "Supervised Model";
    ViewData["SubHead"] = "Learn about the burials through a";
    ViewData["Head"] = "Supervised Model";
    ViewData["Button"] = "Input Data";
    ViewData["Btn"] = "btn btn-primary btn - xl text - uppercase";
}

@model CombinedInputData
<!-- Services-->
<br />
<br />
<section class="page-section bg-light" id="services">
    <div class="container">
        <div>
            <h2>Predict Wrapping Type: </h2>
            <!-- Add your form for user input -->
            <form asp-controller="Home" asp-action="PredictWrapping" method="post">
                <div class="form-group">
                    <label for="WrappingInputData.Depth">Depth: </label>
                    <input asp-for="WrappingInputData.Depth" class="form-control" id="WrappingInputData.Depth" name="WrappingInputData.Depth">
                </div>
                <div class="form-group">
                    <label for="WrappingInputData.Depth">Length:</label>
                    <input asp-for="WrappingInputData.Length" class="form-control" id="WrappingInputData.Length" name="WrappingInputData.Length">
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Headdirection_W" class="form-check-input" id="WrappingInputData.Headdirection_W" name="WrappingInputData.Headdirection_W">
                    <label class="form-check-label" for="WrappingInputData.Headdirection_W">Head Direction West</label>
                </div>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" >
                    <label class="form-check-label">Head Direction East</label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Facebundles_Y" class="form-check-input" id="WrappingInputData.Facebundles_Y" name="WrappingInputData.Facebundles_Y">
                    <label class="form-check-label" for="WrappingInputData.Facebundles_Y">Facebundle? (Y/N) </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Goods_Y" class="form-check-input" id="WrappingInputData.Goods_Y" name="WrappingInputData.Goods_Y">
                    <label class="form-check-label" for="WrappingInputData.Goods_Y">Goods Found? (Y/N) </label>
                </div>
                <span>Hair Color: </span>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Haircolor_B" class="form-check-input" id="WrappingInputData.Haircolor_B" name="WrappingInputData.Haircolor_B">
                    <label class="form-check-label" for="WrappingInputData.Haircolor_B">Brown/Dark Brown/Lt Brown </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Haircolor_D" class="form-check-input" id="WrappingInputData.Haircolor_D" name="WrappingInputData.Haircolor_D">
                    <label class="form-check-label" for="WrappingInputData.Haircolor_D">Blonde </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Haircolor_K" class="form-check-input" id="WrappingInputData.Haircolor_K" name="WrappingInputData.Haircolor_K">
                    <label class="form-check-label" for="WrappingInputData.Haircolor_K">Black </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Haircolor_R" class="form-check-input" id="WrappingInputData.Haircolor_R" name="WrappingInputData.Haircolor_R">
                    <label class="form-check-label" for="WrappingInputData.Haircolor_R">Red/Red-Bl </label>
                </div>
                <div class="form-check">
                    <input  type="checkbox" class="form-check-input">
                    <label class="form-check-label">Brown-Red </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Haircolor_unknown" class="form-check-input" id="WrappingInputData.Haircolor_unknown" name="WrappingInputData.Haircolor_unknown">
                    <label class="form-check-label" for="WrappingInputData.Haircolor_unknown">Unknown Hair Color </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Samplescollected_true" class="form-check-input" id="WrappingInputData.Samplescollected_true" name="WrappingInputData.Samplescollected_true">
                    <label class="form-check-label" for="WrappingInputData.Samplescollected_true">Samples Collected? (Y/N) </label>
                </div>
                <span>Age At Death: </span>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Ageatdeath_C" class="form-check-input" id="WrappingInputData.Ageatdeath_C" name="WrappingInputData.Ageatdeath_C">
                    <label class="form-check-label" for="WrappingInputData.Ageatdeath_C">Adult</label>
                </div>
                <div class="form-check">
                    <input  type="checkbox" class="form-check-input">
                    <label class="form-check-label">Child </label>
                </div>
                <div class="form-check">
                    <input asp-for="WrappingInputData.Ageatdeath_unknown" class="form-check-input" id="WrappingInputData.Ageatdeath_unknown" name="WrappingInputData.Ageatdeath_unknown">
                    <label class="form-check-label" for="WrappingInputData.Ageatdeath_unknown">Age at Death Unknown </label>
                </div>
                <!-- Add the rest of the checkbox inputs -->
                @if (ViewBag.WrappingPrediction != null)
                {
                    <h3>Wrapping Prediction: @ViewBag.WrappingPrediction</h3>
                }
                <button type="submit" class="btn btn-primary">Predict</button>
            </form>
        </div>
    </div>
</section>
<br />
